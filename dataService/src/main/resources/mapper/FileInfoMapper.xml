<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.quickmap.dataService.dao.FileInfoMapper">
        <select id="queryFileNameSet" resultType="String">
          SELECT DISTINCT f.filename FROM file_info f WHERE f.isdel = 1
        </select>
</mapper>